            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Dashpage" />
        </activity>
        <activity
            android:name="com.a3x3conect.mobile.isthreeinjava.Support"
            android:parentActivityName=".Dashpage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
